{
	"name": "sveltese",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"prisma": "prisma generate && prisma migrate dev --name init --preview-feature",
		"prisma:generate": "prisma generate",
		"prisma:migrate": "prisma migrate dev --name init --preview-feature",
		"prisma:studio": "prisma studio",
		"prisma:reset": "prisma migrate reset --preview-feature",
		"prisma:seed": "prisma db seed --preview-feature",
		"prisma:push": "prisma migrate push --preview-feature"
	},
	"devDependencies": {
		"@playwright/test": "^1.34.3",
		"@sveltejs/adapter-auto": "^2.1.0",
		"@sveltejs/kit": "^1.20.2",
		"@types/node": "^18.16.16",
		"@types/nodemailer": "^6.4.8",
		"@typescript-eslint/eslint-plugin": "^5.59.9",
		"@typescript-eslint/parser": "^5.59.9",
		"autoprefixer": "^10.4.14",
		"d3": "^7.8.5",
		"d3-scale": "^4.0.2",
		"dotenv": "^16.1.4",
		"eslint": "^8.42.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"postcss": "^8.4.24",
		"postcss-load-config": "^4.0.1",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.1",
		"prisma": "^4.15.0",
		"prisma-zod-generator": "^0.8.13",
		"svelte": "^3.59.1",
		"svelte-check": "^3.4.3",
		"svelte-preprocess": "^4.10.7",
		"sveltekit-superforms": "^0.7.1",
		"tailwindcss": "^3.3.2",
		"ts-node": "^10.9.1",
		"tslib": "^2.5.3",
		"typescript": "^4.9.5",
		"vite": "^4.3.9",
		"vitest": "^0.25.8",
		"zod": "^3.21.4"
	},
	"type": "module",
	"dependencies": {
		"@lucia-auth/adapter-prisma": "^2.0.0",
		"@prisma/client": "4.15.0",
		"@vercel/analytics": "^1.0.1",
		"class-variance-authority": "^0.6.0",
		"clsx": "^1.2.1",
		"lucia-auth": "^1.7.1",
		"lucide-svelte": "^0.236.0",
		"nodemailer": "^6.9.3",
		"radix-svelte": "^0.7.1",
		"svelte-email": "^0.0.4",
		"tailwind-merge": "^1.13.0",
		"tailwindcss-animate": "^1.0.5"
	}
}
