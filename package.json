{
	"name": "sveltese",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"prisma": "prisma generate && prisma migrate dev --name init --preview-feature",
		"prisma:generate": "prisma generate",
		"prisma:migrate": "prisma migrate dev --name init --preview-feature",
		"prisma:studio": "prisma studio",
		"prisma:reset": "prisma migrate reset --preview-feature",
		"prisma:seed": "prisma db seed --preview-feature",
		"prisma:push": "prisma migrate push --preview-feature"
	},
	"devDependencies": {
		"@playwright/test": "^1.33.0",
		"@sveltejs/adapter-auto": "^2.0.1",
		"@sveltejs/kit": "^1.16.0",
		"@types/node": "^18.16.3",
		"@types/nodemailer": "^6.4.7",
		"@typescript-eslint/eslint-plugin": "^5.59.2",
		"@typescript-eslint/parser": "^5.59.2",
		"autoprefixer": "^10.4.14",
		"dotenv": "^16.0.3",
		"eslint": "^8.39.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"postcss": "^8.4.23",
		"postcss-load-config": "^4.0.1",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.0",
		"prisma": "^4.13.0",
		"prisma-zod-generator": "^0.8.11",
		"svelte": "^3.58.0",
		"svelte-check": "^3.3.0",
		"svelte-preprocess": "^4.10.7",
		"sveltekit-superforms": "^0.7.1",
		"tailwindcss": "^3.3.2",
		"ts-node": "^10.9.1",
		"tslib": "^2.5.0",
		"typescript": "^4.9.5",
		"vite": "^4.3.4",
		"vitest": "^0.25.8",
		"zod": "^3.21.4"
	},
	"type": "module",
	"dependencies": {
		"@lucia-auth/adapter-prisma": "^2.0.0",
		"@lucia-auth/oauth": "^1.1.1",
		"@prisma/client": "4.13.0",
		"@vercel/analytics": "^1.0.1",
		"lucia-auth": "^1.5.0",
		"nodemailer": "^6.9.1",
		"svelte-email": "^0.0.4"
	}
}
